#Initialize Matrix bbb which contains the status of a Tic Tac Toe game
a <- c("O", NA, "X")
b <- c(NA, "O", "O")
c <- c("X", NA, "X" )

bbb <- matrix(c(a, b, c), nrow = 3, byrow = TRUE)

